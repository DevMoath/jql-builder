{
    "name": "devmoath/jql-builder",
    "description": "Simple JQL builder for Jira search",
    "keywords": [
        "devmoath",
        "jql-builder",
        "php",
        "builder",
        "jira",
        "query-builder",
        "jql"
    ],
    "homepage": "https://github.com/devmoath/jql-builder",
    "support": {
        "issues": "https://github.com/devmoath/jql-builder/issues",
        "source": "https://github.com/devmoath/jql-builder"
    },
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Moath Alhajri",
            "email": "moath.alhajrii@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/macroable": "^2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.1",
        "nunomaduro/collision": "^5.9",
        "phpstan/phpstan": "^0.12.99",
        "phpunit/phpunit": "^9.5.9"
    },
    "autoload": {
        "psr-4": {
            "DevMoath\\JqlBuilder\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DevMoath\\JqlBuilder\\Tests\\": "tests"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "XDEBUG_MODE=coverage phpunit --coverage-html coverage",
        "php-cs-fixer": "php-cs-fixer fix --dry-run",
        "php-cs-fixer:fix": "php-cs-fixer fix",
        "phpstan": "phpstan analyse"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
